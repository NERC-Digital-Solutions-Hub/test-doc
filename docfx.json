{
  "_disableToc": false,
  "$schema": "https://raw.githubusercontent.com/dotnet/docfx/main/schemas/docfx.schema.json",
  "metadata": [
    {
      "src": [
        {
          "src": "C:\\NDSH\\GitHub\\ndsh-dotnet-temp\\dev\\Proj\\src\\src",
          "files": [
            "**/*.csproj"
          ]
        }
      ],
      "dest": "api",
      "allowCompilationErrors": true,
      "namespaceLayout": "nested",
      "categoryLayout": "nested"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "**/*.{md,yml}"
        ],
        "exclude": [
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "output": "_site",
    "template": [
      "default",
      "modern"
    ],
    "globalMetadata": {
      "_appName": "NDSH",
      "_appTitle": "NDSH",
      "_enableSearch": true,
      "pdf": false
    }
  }
}